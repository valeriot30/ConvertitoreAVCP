<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.RadioButton?>

<?import javafx.scene.layout.AnchorPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.pointservice.demo.controller.DatabaseController"
      prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="successMessage" style="-fx-text-background-color: green;"/>
    <Label fx:id="errorMessage" style="-fx-text-background-color: red;"/>
    <Label fx:id="inputText"/>

    <Label text="Nome del database"></Label>
    <TextField></TextField>
    <Button text="Converti" onAction="#onStartConvert"/>
    <Button text="Indietro" onAction="#onBackButton"/>
</VBox>
